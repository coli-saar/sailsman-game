{
  "title": "Room",
  "html": [
    {
      "layout-type": "div",
      "id": "typing",
      "style": "margin: 5px 10px; min-height: 20px",
      "layout-content": [
        {
          "layout-type": "div",
          "style": "padding: 10px; display: flex",
          "layout-content": [
            {
              "layout-type": "button",
              "class": "button",
              "role": "button",
              "layout-content": "submit",
              "id": "submit_button",
              "style": "padding: 10px; margin: 5px; width: 100%"
            }
          ]
        }
      ]
    },
    {
      "layout-type": "div",
      "class": "card",
      "layout-content": [
        {
          "layout-type": "div",
          "layout-content": [
            {
              "layout-type": "b",
              "layout-content": [
                {
                  "layout-type": "lighter",
                  "id": "instr_title",
                  "style": "min-height: 20px"
                }
              ]
            }
          ],
          "style": "color: #abb2b9;"
        },
        {
          "layout-type": "hr",
          "layout-content": "",
          "style": "margin: 2px; background-color: #abb2b9; opacity: .50;"
        },
        {
          "layout-type": "div",
          "id": "instr",
          "style": "white-space: pre-wrap;"
        },
        {
          "layout-type": "div",
          "class": "container",
          "style": "display: flex; justify-content: center; align-items: flex-start;",
          "layout-content": [
            {
              "layout-type": "div",
              "id": "graph-area",
              "style": "position: relative; display: flex; flex-direction: row; align-items: top; justify-content: center; width: 90%; height: 100%; margin: 0px; text-align: center;",
              "layout-content": [
                {
                  "layout-type": "div",
                  "id": "acc-weights-div",
                  "style": "position: absolute; display: flex; align-items: center; flex: 0 1 auto; height: min-content;"
                },
                {
                  "layout-type": "svg",
                  "id": "graph-svg",
                  "style": "aspect-ratio: 1024/700; width: 90%; height: auto; max-width: 100vh; margin: 0;"
                },
                {
                  "layout-type": "button",
                  "class": "button",
                  "role": "button",
                  "layout-content": "reset graph",
                  "id": "reset-graph-button",
                  "style": "margin-left: 5px; width: 10%; aspect-ratio: 7/9;"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "css": {
    ".button": {
      "text-transform": "uppercase",
      "font-size": "medium",
      "border-radius": "5px",
      "border": "none",
      "box-shadow": "rgba(0,0,0,0.14902) 0 1px 1px 0,rgba(0,0,0,0.09804) 0 1px 2px 0",
      "outline": 0,
      "background": "#00305E",
      "width": "100%",
      "color": "#FFFFFF",
      "padding": "15px",
      "cursor": "pointer"
    },
    ".button:hover": {
      "content": "''",
      "background": "#818BAC"
    },
    "#reset-graph-button": {
      "display": "flex",
      "font-size": "1.3em",
      "justify-content": "center",
      "align-items": "center",
      "background": "rgb(255, 226, 205)",
      "color": "rgb(230, 92, 0)",
      "padding": "15px",
      "margin-top": "20px",
      "width": "150px",
      "height": "90px",
      "border": "none",
      "cursor": "pointer",
      "border-radius": "5px",
      "box-shadow": "rgba(230, 92, 0, 1) 1px 4px 3px 0",
      "transition": "all 0.1s ease-in-out"
    },
    "#reset-graph-button:active": {
      "transform": "translateY(2px)",
      "box-shadow": "rgba(230, 92, 0, 1) 0 1px 2px 0"
    },
    "#reset-graph-button:active:after": {
      "content": "''",
      "position": "absolute",
      "top": "0",
      "left": "0",
      "width": "100%",
      "height": "100%",
      "background": "rgba(255, 186, 133, 0.2)",
      "border-radius": "5px"
    },
    "#reset-graph-button:hover": {
      "font-size": "1.1em"
    }
  },
  "scripts": {
    "incoming-text": "markdown",
    "incoming-image": "display-image",
    "submit-message": "send-message",
    "print-history": "markdown-history",
    "plain": [
      "d3.v7.min",
      "graph_d3"
    ]
  },
  "show_latency": false
}
